{
  "name": "@bitaccess/coinlib-stellar",
  "version": "5.0.5",
  "description": "Library to assist in processing stellar payments, such as deriving addresses and sweeping funds",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.umd.js",
  "types": "dist/lib/index.d.ts",
  "esnext": "dist/lib/index.js",
  "repository": "https://github.com/bitaccess/coinlib/tree/master/packages/coinlib-stellar",
  "homepage": "https://github.com/bitaccess/coinlib/tree/master/packages/coinlib-stellar#readme",
  "bugs": {
    "url": "https://github.com/bitaccess/coinlib/issues"
  },
  "keywords": [
    "coin",
    "payments",
    "crypto",
    "ripple",
    "xrp",
    "bitaccess"
  ],
  "author": "Dylan Seago <dylan@bitaccess.co>",
  "license": "MIT",
  "files": [
    "dist/",
    "src/"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "bin/docs.sh",
    "prebuild": "rimraf dist && rimraf docs",
    "build": "bin/build.sh",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --runInBand --detectOpenHandles --bail",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "version": "npm run build",
    "sandbox": "bin/ts-node.sh test/sandbox.ts"
  },
  "devDependencies": {
    "@faast/ts-config": "^1.3.5",
    "@types/bip39": "^2.4.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.144",
    "@types/node": "^14.0.24",
    "@types/node-fetch": "^2.5.0",
    "@types/promise-retry": "^1.1.3",
    "coveralls": "^3.0.2",
    "jest": "^27.2.4",
    "jest-circus": "^27.2.4",
    "jest-config": "^27.2.4",
    "lint-staged": "^8.0.0",
    "node-fetch": "^2.6.1",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^2.27.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@bitaccess/coinlib-common": "^5.0.1",
    "@faast/ts-common": "^0.6.0",
    "base-x": "^3.0.6",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "io-ts": "^1.10.4",
    "lodash": "^4.17.15",
    "promise-retry": "^1.1.1",
    "stellar-hd-wallet": "0.0.10",
    "stellar-sdk": "^4.0.1"
