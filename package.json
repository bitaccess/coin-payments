{
  "name": "@bitaccess/coinlib",
  "version": "0.17.2",
  "description": "Unified interface for sending and receiving crypto payments",
  "main": "packages/coin-payments/dist/index.cjs.js",
  "module": "packages/coin-payments/dist/index.es.js",
  "browser": "packages/coin-payments/dist/index.umd.js",
  "types": "packages/coin-payments/dist/lib/index.d.ts",
  "esnext": "packages/coin-payments/dist/lib/index.js",
  "repository": "https://github.com/bitaccess/coin-payments/tree/master/packages/coin-payments",
  "keywords": [
    "coin",
    "payments",
    "crypto",
    "faast",
    "bitaccess"
  ],
  "author": "Dylan Seago <dylan@bitaccess.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitaccess/coinlib/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "install:all": "lerna add",
    "remove:all": "lerna exec npm uninstall",
    "lint": "bin/find-bad-imports.sh && lerna run --parallel lint",
    "format": "lerna run --parallel format",
    "docs": "lerna run --parallel docs",
    "prebuild": "bin/find-bad-imports.sh && lerna run --parallel prebuild",
    "build": "bin/buildall.sh",
    "pretest": "bin/find-bad-imports.sh && lerna run --parallel lint",
    "test": "lerna run --no-bail --ignore @bitaccess/coinlib --ignore @bitaccess/coinlib-ripple --parallel test",
    "preversion": "echo 'Use lerna version instead' && exit 1",
    "release": "npm run bs && lerna publish from-git",
    "link": "lerna link convert && link-parent-bin",
    "postinstall": "link-parent-bin",
    "clean-node-modules": "lerna exec -- rm -rf node_modules && rm -rf node_modules"
  },
  "devDependencies": {
    "@faast/ts-common": "^0.6.0",
    "@faast/ts-config": "^1.3.5",
    "@types/bip39": "^2.4.2",
    "@types/bs58": "^4.0.1",
    "@types/elliptic": "^6.4.10",
    "@types/jest": "^27.0.2",
    "@types/jssha": "^2.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.0.24",
    "@types/node-fetch": "^2.5.0",
    "@types/promise-retry": "^1.1.3",
    "@types/qs": "^6.9.0",
    "@types/request-promise-native": "^1.0.17",
    "coveralls": "^3.0.2",
    "ganache-core": "^2.13.2",
    "husky": "^3.0.9",
    "jest": "^27.2.4",
    "jest-circus": "^27.2.4",
    "jest-config": "^27.2.4",
    "lerna": "^3.18.3",
    "link-parent-bin": "^2.0.0",
    "lint-staged": "^8.0.0",
    "nock": "^11.7.1",
    "node-fetch": "^2.6.1",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^2.27.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^4.4.4",
    "web3-core": "^1.3.6"
  },
  "dependencies": {
    "@bitaccess/coinlib": "file:packages/coinlib",
    "@bitaccess/coinlib-bitcoin": "file:packages/coinlib-bitcoin",
    "@bitaccess/coinlib-bitcoin-cash": "file:packages/coinlib-bitcoin-cash",
    "@bitaccess/coinlib-common": "file:packages/coinlib-common",
    "@bitaccess/coinlib-doge": "file:packages/coinlib-doge",
    "@bitaccess/coinlib-ethereum": "file:packages/coinlib-ethereum",
    "@bitaccess/coinlib-litecoin": "file:packages/coinlib-litecoin",
    "@bitaccess/coinlib-ripple": "file:packages/coinlib-ripple",
    "@bitaccess/coinlib-stellar": "file:packages/coinlib-stellar",
    "@bitaccess/coinlib-tron": "file:packages/coinlib-tron"
  }
}
